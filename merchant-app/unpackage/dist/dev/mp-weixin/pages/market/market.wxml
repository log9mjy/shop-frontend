<view class="page"><view class="page-head" style="{{('height:'+(l+50)+'px')}}"><image class="page-head-bg" style="{{('height:'+(l+50)+'px')}}" src="../../static/image/home-bg.png"></image><view class="head-title-view" style="{{('top:'+(l+10)+'px')}}"><view class="head-title-wrapper"><text class="head-title">配货中心</text></view></view></view><scroll-view class="content-container" scroll-y="{{true}}"><view class="goods-list-view"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['linkDetail',['$0'],[[['goodsList','',index,'id']]]]]]]}}" class="goods-wrapper" bindtap="__e"><view><image class="goods-image" src="{{item.thumbnail}}"></image></view><view class="goods-info"><text>{{item.name}}</text></view><view class="goods-bottom"><text class="goods-price">{{item.price}}</text><image class="add-icon" src="../../static/icon/icon-addd.png" data-event-opts="{{[['tap',[['addStorage',['$event']]]]]}}" catchtap="__e"></image></view></view></block></view></scroll-view><view data-event-opts="{{[['tap',[['linkSto']]]]}}" class="sto-view" bindtap="__e"><view class="sto-wrapper"><image class="sto-image" src="../../static/icon/24gl-bag.png"></image><view class="sto-num">5</view></view></view></view>